{
    "name": "library-example",
    "version": "1.0.0",
    "description": "TS Library",
    "main": "cjs/index.js",
    "module": "es/index.js",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "build:clean": "tsc --build --clean",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "lint": "eslint 'src/**'",
        "lint:fix": "eslint 'src/**/*.*' --fix",
        "dev:watch": "npm-run-all --parallel build:watch dev:lint-fix",
        "dev:lint-fix": "onchange 'src/**/*.*' -e 'src/**/*.(js|d.ts|spec.tsx|scss)' -- npm-run-all --parallel lint:fix",
        "prettier:fix": "prettier --write 'src/**/*.*'",
        "postinstall": "npm run build",
        "compile-ts": "npm run build",
        "compile-non-ts": "copyfiles \"$npm_config_filedest\" es"
    },
    "jest-junit": {
        "outputDirectory": "test-results",
        "outputName": "unit-test-results.xml",
        "uniqueOutputName": "false",
        "ancestorSeparator": " â€º ",
        "usePathForSuiteName": "true"
    },
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@testing-library/jest-dom": "5.7.0",
        "@testing-library/react": "^10.0.2",
        "@testing-library/user-event": "^10.0.1",
        "@types/classnames": "^2.2.9",
        "@types/jest": "^24.9.0",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@typescript-eslint/eslint-plugin": "^2.15.0",
        "@typescript-eslint/parser": "^2.15.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jest": "^23.6.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^1.7.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.1.0",
        "jest-junit": "10.0.0",
        "jest-transform-css": "^2.0.0",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.5",
        "onchange": "^6.1.0",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.19.1",
        "react-test-renderer": "^16.12.0",
        "sass-loader": "^8.0.2",
        "stylelint": "^13.0.0",
        "stylelint-config-standard": "^19.0.0",
        "ts-jest": "^25.1.0",
        "typescript": "^3.9.1"
    },
    "peerDependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    },
    "dependencies": {}
}